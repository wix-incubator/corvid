{
  "name": "corvid-cli",
  "version": "0.2.0",
  "description": "Download your Wix site, code in a local IDE, collaborate, use git, and more!",
  "main": "index.js",
  "scripts": {
    "build": ":",
    "test": "jest",
    "watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-incubator/corvid"
  },
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "corvid": "./src/index.js",
    "corvid-debug": "./src/index.debug.js",
    "corvid-xpert": "./src/index.xpert.js"
  },
  "license": "MIT",
  "devDependencies": {
    "corvid-fake-local-mode-editor": "0.2.0",
    "corvid-local-test-utils": "0.2.0",
    "fetch-mock": "7.3.1",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "wix-eventually": "^2.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.+(e2e|spec|unit).js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupJest.js"
    ]
  },
  "dependencies": {
    "chalk": "2.4.2",
    "corvid-local-logger": "0.2.0",
    "corvid-local-server": "0.2.0",
    "corvid-local-site": "0.2.0",
    "electron": "^5.0.8",
    "es6-error": "^4.1.1",
    "fs-extra": "^7.0.1",
    "has-ansi": "^3.0.0",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.3.0",
    "normalize-url": "^4.2.0",
    "opn": "^5.5.0",
    "ora": "3.4.0",
    "socket.io": "2.2.0",
    "socket.io-client": "^2.2.0",
    "update-notifier": "^3.0.0",
    "yargs": "^13.2.2"
  },
  "peerDependencies": {
    "corvid-types": "^0.1.82"
  }
}
