{
  "env": {
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
    "import/no-extraneous-dependencies": "error"
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended",  "plugin:prettier/recommended", "plugin:import/errors", "plugin:import/warnings"]
}